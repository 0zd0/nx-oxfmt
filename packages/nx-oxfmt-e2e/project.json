    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "packages/nx-oxfmt-e2e/src",
    "implicitDependencies": ["nx-oxfmt"],
    "targets": {
        "e2e": {
            "executor": "@nx/jest:jest",
